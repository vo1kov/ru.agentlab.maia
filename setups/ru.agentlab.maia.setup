<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:mylyn="http://www.eclipse.org/oomph/setup/mylyn/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/mylyn/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Mylyn.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="ru.agentlab.maia"
    label="MAIA">
  <setupTask
      xsi:type="git:GitCloneTask"
      id="ru.agentlab.maia.git.clone"
      remoteURI="agentlab/ru.agentlab.maia"
      pushURI="">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Github repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.targlets:TargletTask">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}"
        includeAllPlatforms="true">
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
        <detail
            key="location">
          <value>${ru.agentlab.maia.git.clone.location/releng/ru.agentlab.maia.releng/ru.agentlab.maia.releng.target}</value>
        </detail>
        <detail
            key="includeAllPlatforms">
          <value>true</value>
        </detail>
        <detail
            key="includeSource">
          <value>true</value>
        </detail>
        <detail
            key="generateVersions">
          <value>false</value>
        </detail>
        <detail
            key="generateImplicitUnits">
          <value>true</value>
        </detail>
        <detail
            key="includeMode">
          <value>slicer</value>
        </detail>
      </annotation>
      <requirement
          name="com.google.gerrit.common"/>
      <requirement
          name="com.google.gson"/>
      <requirement
          name="com.google.inject"/>
      <requirement
          name="com.google.inject.assistedinject"/>
      <requirement
          name="com.google.inject.multibindings"/>
      <requirement
          name="gov.nist.math.jama.gov.nist.math.jama"/>
      <requirement
          name="javax.annotation"/>
      <requirement
          name="javax.inject"/>
      <requirement
          name="net.sf.trove4j.feature.feature.group"/>
      <requirement
          name="org.apache.commons.io"/>
      <requirement
          name="org.apache.commons.lang3"/>
      <requirement
          name="org.apache.commons.logging"/>
      <requirement
          name="org.apache.commons.math"/>
      <requirement
          name="org.apache.commons.math3"/>
      <requirement
          name="org.aspectj.feature.group"/>
      <requirement
          name="org.eclipse.ajdt.feature.group"/>
      <requirement
          name="org.eclipse.equinox.weaving.sdk.feature.group"/>
      <requirement
          name="org.eclipse.xtext.xbase.lib.feature.group"/>
      <requirement
          name="org.kohsuke.args4j"/>
      <requirement
          name="org.objenesis"/>
      <requirement
          name="org.sat4j.core"/>
      <requirement
          name="org.semanticweb.owlapi.feature.feature.group"/>
      <requirement
          name="*"/>
      <sourceLocator
          rootFolder="${ru.agentlab.maia.git.clone.location}/features"
          locateNestedProjects="true"/>
      <sourceLocator
          rootFolder="${ru.agentlab.maia.git.clone.location}/plugins"
          locateNestedProjects="true"/>
      <sourceLocator
          rootFolder="${ru.agentlab.maia.git.clone.location}/releng"
          locateNestedProjects="true"/>
      <sourceLocator
          rootFolder="${ru.agentlab.maia.git.clone.location}/setups"
          locateNestedProjects="true"/>
      <repositoryList
          name="Neon">
        <repository
            url="http://agentlab.ru/pd/p2/eclipseorbit/"/>
        <repository
            url="http://agentlab.ru/p2/orbit/org.semanticweb.owlapi/updates/nightly/latest/"/>
        <repository
            url="http://download.eclipse.org/tools/ajdt/45/dev/update"/>
        <repository
            url="http://agentlab.ru/p2/orbit/net.sf.trove4j/drops/nightly/3.0.3.201602051202/"/>
        <repository
            url="http://download.eclipse.org/modeling/tmf/xtext/updates/composite/releases/"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="mylyn:MylynQueriesTask"
      connectorKind="github"
      repositoryURL="https://github.com/agentlab/ru.agentlab.maia"
      userID="${github.user.id}"
      password="${github.user.password}">
    <query summary="${scope.project.label} All Open Tasks">
      <attribute
          key="state"
          value="open::"/>
    </query>
    <query summary="${scope.project.label} My Open Tasks">
      <attribute
          key="state"
          value="open::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
    <query summary="${scope.project.label} My Closed Tasks"
        url="">
      <attribute
          key="state"
          value="closed::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"
      id="InitialBuild"
      refresh="true">
    <description>Initial Build</description>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="${scope.project.label}">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/${scope.project.name}/.*"/>
    </workingSet>
    <description></description>
  </setupTask>
  <stream name="develop"
      label=""/>
  <stream name="master"/>
  <logicalProjectContainer
      xsi:type="setup:Project"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='ru.agentlab']/@projects[name='osgi']"/>
  <description>IDE Setup for MAIA Project</description>
</setup:Project>
